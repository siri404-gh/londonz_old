{
  "expo": {
    "name": "sreeram.app",
    "description": "Sreeram's App",
    "slug": "sreeramapp",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/mobile/assets/icon.png",
    "splash": {
      "image": "./src/mobile/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sreeram.app"
    },
    "android": {
      "package": "com.sreeram.app"
    }
  }
}